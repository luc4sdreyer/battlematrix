    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="point">
    <xs:sequence>
      <xs:element name="x" type="xs:int"/>
      <xs:element name="y" type="xs:int"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="unitEvent">
    <xs:sequence>
      <xs:element minOccurs="0" name="bullet" type="tns:bullet"/>
